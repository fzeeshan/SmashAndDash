if shake
{

//change direction
    if curshake &gt;= maxshake
        {goright = false}  
    if curshake &lt;= -maxshake
        {goright = true}

    if goright
        {curshake += maxshake}
    else
        {curshake -= maxshake}
 
    //this function below makes sure there is no zero shake value
    if curshake == 0
        {if goright
            {curshake += maxshake}
        else{curshake -= maxshake}}

    //change x (shake)
    view_xview[0] = curshake
    shake_length -= 1
 
    //quake reset
    if shake_length &lt;= 0
        {view_xview[0] = 0
        curshake = 0}
}  
</string>
